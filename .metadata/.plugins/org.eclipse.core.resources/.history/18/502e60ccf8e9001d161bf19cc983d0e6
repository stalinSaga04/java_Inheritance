package usingWhileLoops;

public class PrimeNumber {
	public static void main(String[] args) {
		PrimeNumber pn = new PrimeNumber();
		pn.find_prime(50);
	} 
	void find_prime(int num) {
		int i = 2;
		int count =0;
		while (i <= num/2) {
			if(num%i !=0 & num%3 !=0) {
				
			System.out.print(num + " ");
		}
			num=num-1;
			count = count+1;
			
		}
		System.out.println(count);
	}

}
