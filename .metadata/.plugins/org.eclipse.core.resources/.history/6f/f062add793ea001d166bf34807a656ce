package usingWhileLoops;

public class PrimeNumber {
	public static void main(String[] args) {
	
		PrimeNumber pn = new PrimeNumber();
		pn.find_prime_number(50);
		
	}
	void find_prime_number(int num) {
		int count=0,init =2;
		while(init<=num/2){
			if(num%init==0) {
				count=count+1;
				System.out.print(num+" ");
				init++;
			}
			
			
		}
		
	}

}
