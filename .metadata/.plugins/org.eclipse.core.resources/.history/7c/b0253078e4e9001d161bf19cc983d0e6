package usingWhileLoops;

public class MultiplicationOf3 {
	static int res ;
	public static void main(String [] args) {
		MultiplicationOf3 mul = new MultiplicationOf3();
		mul.mul_of_three(5);
		
	}
	  int mul_of_three(int num) {
		  int count = num;
		  while(num>0) {
			 res=(count-num+1)*3;
			  num = num-1;
			  System.out.print(res);
		  }
		  return res;
	}

}
