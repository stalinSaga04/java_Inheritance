package usingWhileLoops;

public class PrimeNumber {
	public static void main(String[] args) {
		PrimeNumber pn = new PrimeNumber();
		pn.find_prime(50);
	} 
	void find_prime(int num) {
		int i = 2,count =0;
		while (i <= num/2) {
			if(num%i == 0) {
			count= count+1;
			//break;
		}
			
			i = i+1;
			
			
		}
		if (count == 0) {
			System.out.println(num);
		}
		num = num++;
		//System.out.println(count);
	}

}
