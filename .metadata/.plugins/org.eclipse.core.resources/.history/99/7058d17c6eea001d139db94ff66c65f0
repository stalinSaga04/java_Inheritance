package usingWhileLoops;

public class MultiplicationOf3 {
	static int res ;
	public static void main(String [] args) {
		MultiplicationOf3 mul = new MultiplicationOf3();
		mul.mul_of_three(10);
		
	}
	  int mul_of_three(int num) {
		  int count = num;
		  while(num>0) {
			  if (num%3==0 || num%5==0) {
				System.out.print(num-count+1+" ");
			}
			  num = num-1;
		  }
		  return res;
	}

}
