package usingWhileLoops;

public class PrimeNumber {
	public static void main(String[] args) {
		PrimeNumber pn = new PrimeNumber();
		pn.find_prime(50);
	} 
	void find_prime(int num) {
		int i = 2,count =0;
		while (i <= num/2) {
			if(num%i != 0) {
			System.out.print(num + " ");
			count= count+1;
		}
			
			i = i+1;
			
			
		}
		//System.out.println(count);
	}

}
