package usingWhileLoops;

public class OddAndEven {
	
	public static void main(String [] args) {
		OddAndEven oe = new OddAndEven();
		oe.odd_method(10);
		
	}
	void odd_method(int num){
		int count = num-1;
		while (num!=0) {
			if (num%2!=0) {
				System.out.print(num-count);
			}
			num = num-1;
		}
	}

}

